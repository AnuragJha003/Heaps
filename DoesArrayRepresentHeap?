class Solution{
    public:
    bool isMaxHeap(int a[], int n)
    {
        // Your code goes here
        for(int i=0;i<=(n/2)-1;i++) 

        {

            if((a[i]<a[(2*i+1)] || a[i]<a[(2*i+2)]))  

               return false;

            

        }

        return true;
    }
};

